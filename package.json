{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev:client": "vite dev --port 5000",
    "dev": "cross-env NODE_ENV=development tsx server/index-dev.ts",
    "build": "vite build && esbuild server/index-prod.ts --platform=node --packages=external --bundle --format=esm --outfile=dist/index.mjs",
    "start": "node dist/index.mjs",
    "check": "tsc",
    "lint": "eslint . --max-warnings 549",
    "lint:strict": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,css,md}\"",
    "db:push": "drizzle-kit push",
    "test": "npx vitest run",
    "test:watch": "npx vitest",
    "test:ci": "cross-env CI=true npx vitest run --reporter=verbose --reporter=junit --outputFile=test-results.xml",
    "test:coverage": "npx vitest run --coverage",
    "test:coverage:ci": "cross-env CI=true npx vitest run --coverage --reporter=verbose",
    "test:client": "npx vitest run --config vitest.client.config.ts",
    "test:server": "npx vitest run --config vitest.server.config.ts",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "analyze": "cross-env ANALYZE=true npx vite build",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "prepare": "husky || echo \"husky skipped\""
  },
